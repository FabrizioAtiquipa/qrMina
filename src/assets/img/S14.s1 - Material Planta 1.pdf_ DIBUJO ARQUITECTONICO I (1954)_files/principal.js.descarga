							if ($('#valorar-recurso').length != 1 && $('#valorar-pagina').length != 1 && not_match_init_page_but_others) {
								$('#content').prepend(html);
							}
							$('#valorar-recurso').prepend(html2);
							$('#valorar-pagina').prepend(html3);
