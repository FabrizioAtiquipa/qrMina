    quiz_re = new RegExp('https:\/\/(.*?)\/courses\/([0-9]*)\/quizzes\/([0-9]*)$', 'g');
    full_quiz_re = RegExp('https:\/\/(.*?)\/courses\/([0-9]*)\/quizzes\/([0-9]*)\/(statistics|read_only)$', 'g');
    if (str.match(quiz_re)) {
        $("#preview_quiz_button").remove();
        $("ul.page-action-list li:not(:contains('SpeedGrader'))").remove();
    } else if (str.match(full_quiz_re)) {
        window.location.href = window.location.href.substr(0, window.location.href.lastIndexOf('/'));
    }
}
/************************ Fin Ocultar Preguntas Roles  *************************/

